package com.huatuo.customer.base.request;

import java.io.Serializable;

/**
 * 提交参数父类
 * @author master
 *
 */
public class BaseForm implements Serializable{
	
	private static final long serialVersionUID = -8947574395282469509L;
	/**
	 * 与登录用户对应的令牌
	 */
	private String token;

	public String getToken() {
		return token;
	}

	public void setToken(String token) {
		this.token = token;
	}
	
}
