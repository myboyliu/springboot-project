  <!-- 查询每个医生的号表 -->
  <select id="queryNumTable" parameterType="string" resultType="com.huatuo.customer.domain.NumTablePojo">
	select xr1.week week, xr1.interval 'interval', concat(left(xr1.start_time, 5), "-", left(xr2.end_time, 5)) startEndTime
	from (
		select *
		from (
			select xr.week, xr.interval, xr.start_time
			from xt_reservation xr
			where 1=1
			and xr.doctor_uid = #{doctorUid}
			and xr.is_del = 0
			order by xr.week, xr.interval, xr.start_time asc
			) xr
		group by xr.week, xr.interval
		) xr1
	left join (
		select *
		from (
			select xr.week, xr.interval, xr.end_time
			from xt_reservation xr
			where 1=1
			and xr.doctor_uid = #{doctorUid}
			and xr.is_del = 0
			-- group by xr.week, xr.interval, xr.start_time
			order by xr.week, xr.interval, xr.end_time desc
			) xr
			group by xr.week, xr.interval
		) xr2
	on(xr1.week = xr2.week and xr1.interval = xr2.interval)	
  </select>
