package com.huatuo.customer.query;

import com.huatuo.customer.base.query.BaseQuery;

/**
 * 订单查询封装对象
 * @author master
 *
 */
public class OrderQuery extends BaseQuery{
	
	private static final long serialVersionUID = 1424330534583263655L;
	/**
	 * 登录用户id
	 */
	private Long userId;
	/**
	 * 订单查询状态
	 */
	private Integer orderState;
	/**
	 * 支付订单号
	 */
	private String orderNo;
	/**
	 * 业务订单号
	 */
	private Long visitId;
	/**
	 * 支付方式，1:web,2:安卓,3:ios
	 */
	private Integer payType;
	/**
	 * 支付来源，2，支付宝，3微信
	 */
	private Integer paySource;
	
	public Integer getPaySource() {
		return paySource;
	}
	public void setPaySource(Integer paySource) {
		this.paySource = paySource;
	}
	public Integer getPayType() {
		return payType;
	}
	public void setPayType(Integer payType) {
		this.payType = payType;
	}
	public Long getVisitId() {
		return visitId;
	}
	public void setVisitId(Long visitId) {
		this.visitId = visitId;
	}
	public String getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	public Long getUserId() {
		return userId;
	}
	public void setUserId(Long userId) {
		this.userId = userId;
	}
	public Integer getOrderState() {
		return orderState;
	}
	public void setOrderState(Integer orderState) {
		this.orderState = orderState;
	}
}
